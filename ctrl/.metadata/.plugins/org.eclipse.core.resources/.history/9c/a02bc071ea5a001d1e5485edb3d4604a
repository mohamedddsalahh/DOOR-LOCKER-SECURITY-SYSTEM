#ifndef CTRL_H_
#define CTRL_H_

#define PASS_READY		0x01
#define	CONFIRM_READY	0x02
#define MC2_READY	    0x03
#define CHECK_WAIT		0x04

#define PASS_ADDRESS		0x0311
#define	RECONFIRM_ADDRESS	0x0316

void create_pass(void);


#endif /* CTRL_H_ */
